<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ZLCircleProgressBar">
        <attr name="progress_roundColor" format="color"/>
        <attr name="progress_roundProgressColor" format="color"/>
        <attr name="progress_roundWidth" format="dimension"></attr>
        <attr name="progress_max" format="integer"></attr>
        <attr name="progress_style">
            <enum name="STROKE" value="0"></enum>
            <enum name="FILL" value="1"></enum>
        </attr>
    </declare-styleable>
</resources>